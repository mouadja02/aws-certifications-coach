# AWS Free Tier Configuration Example
# Copy this file to terraform.tfvars and fill in your values

aws_region  = "us-east-1"
environment = "production"
account_id  = "123456789012"  # Your AWS account ID

# Security: Update this to your IP address
admin_ip = "YOUR_IP_ADDRESS/32"  # e.g., "203.0.113.1/32"

# SSH Key (create in AWS EC2 console first)
ssh_key_name = "aws-coach-key"

# n8n Configuration
n8n_username = "admin"
n8n_password = "CHANGE_THIS_STRONG_PASSWORD"  # Min 16 characters
