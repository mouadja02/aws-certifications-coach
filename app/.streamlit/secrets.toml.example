# Streamlit Cloud Secrets Configuration
# Add these secrets in Streamlit Cloud dashboard under Settings > Secrets
# OR create a .streamlit/secrets.toml file locally (don't commit!)

# Snowflake Configuration (Native Streamlit Integration)
[connections.snowflake]
account = "abc12345.us-east-1"
user = "your_username"
password = "your_password"
role = "ACCOUNTADMIN"
warehouse = "COMPUTE_WH"
database = "AWS_CERTIFICATIONS"
schema = "PUBLIC"
client_session_keep_alive = true

# n8n Webhook URLs (optional - for AI features)
[general]
N8N_CHAT_WEBHOOK_URL = "http://YOUR_N8N_IP:5678/webhook/chat"
N8N_EXAM_WEBHOOK_URL = "http://YOUR_N8N_IP:5678/webhook/exam"